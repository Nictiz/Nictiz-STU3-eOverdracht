<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="eOverdracht-Task" />
  <url value="http://nictiz.nl/fhir/StructureDefinition/eOverdracht-Task" />
  <name value="eOverdrachtTask" />
  <title value="eOverdracht Task" />
  <status value="draft" />
  <publisher value="Nictiz" />
  <contact>
    <name value="Nictiz" />
    <telecom>
      <system value="email" />
      <value value="info@nictiz.nl" />
      <use value="work" />
    </telecom>
  </contact>
  <description value="The use cases within eOverdracht form a workflow that may include the negotiation of the transfer of the patient, and ends with the transfer of the medical record. This Task resource is used to track the worflow and acts as the entry point for all data exchanges within eOverdracht." />
  <copyright value="CC0" />
  <fhirVersion value="3.0.2" />
  <mapping>
    <identity value="eOverdracht-sturen-aanmeldbericht" />
    <uri value="https://decor.nictiz.nl/art-decor/decor-scenarios--e-overdracht-?id=2.16.840.1.113883.2.4.3.11.60.30.4.63" />
    <name value="eOverdracht 4.0 transaction &quot;Sturen aanmeldbericht&quot;" />
  </mapping>
  <mapping>
    <identity value="eOverdracht-sturen-antwoord-aanmeldbericht" />
    <uri value="https://decor.nictiz.nl/art-decor/decor-scenarios--e-overdracht-?id=2.16.840.1.113883.2.4.3.11.60.30.4.64" />
    <name value="eOverdracht 4.0 transaction &quot;Sturen antwoord aanmeldbericht&quot;" />
  </mapping>
  <mapping>
    <identity value="eOverdracht-sturen-bevestiging-of-annulering-aanmelding" />
    <uri value="https://decor.nictiz.nl/art-decor/decor-scenarios--e-overdracht-?id=2.16.840.1.113883.2.4.3.11.60.30.4.68" />
    <name value="eOverdracht 4.0 transaction &quot;Sturen bevestiging of annulering aanmelding&quot;" />
  </mapping>
  <mapping>
    <identity value="hcim-patient-v3.1-2017EN" />
    <uri value="https://zibs.nl/wiki/Patient-v3.1(2017EN)" />
    <name value="HCIM Patient-v3.1(2017EN)" />
  </mapping>
  <kind value="resource" />
  <abstract value="false" />
  <type value="Task" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Task" />
  <derivation value="constraint" />
  <differential>
    <element id="Task.status">
      <path value="Task.status" />
      <short value="draft | requested | received | accepted |" />
      <definition value="The current state of the Task. States that can be used: &#xD;&#xA;- requested: Placer has made the registration available&#xD;&#xA;- received: filler has received the request and is juding the request&#xD;&#xA;- accepted: filler accepts the registration, and can provide the care asked for&#xD;&#xA;- on-hold: filler proposes a different date&#xD;&#xA;- in-progress: placer confirms to the filler it proceeds with the transfer&#xD;&#xA;- rejected: filler rejects the registration&#xD;&#xA;- cancelled: placer or filler has cancelled the transfer&#xD;&#xA;- completed: filler recieved the overdrachtsbericht&#xD;&#xA;- ready,  failed, will not be used" />
      <mapping>
        <identity value="eOverdracht-sturen-antwoord-aanmeldbericht" />
        <map value="e-overdracht-dataelement-v4-1166" />
        <comment value="Antwoord aanmelding" />
      </mapping>
      <mapping>
        <identity value="eOverdracht-sturen-bevestiging-of-annulering-aanmelding" />
        <map value="e-overdracht-dataelement-v4-1167" />
        <comment value="Bevestiging of annulering aanmelding" />
      </mapping>
    </element>
    <element id="Task.statusReason.text">
      <path value="Task.statusReason.text" />
      <mapping>
        <identity value="eOverdracht-sturen-antwoord-aanmeldbericht" />
        <map value="e-overdracht-dataelement-v4-1168" />
        <comment value="Toelichting antwoord aanmelding" />
      </mapping>
    </element>
    <element id="Task.intent">
      <path value="Task.intent" />
      <comment value="This element is immutable.  Proposed tasks, planned tasks, etc. must be distinct instances.&#xA;&#xA;In the case the Task has the intent &quot;order&quot;" />
    </element>
    <element id="Task.code">
      <path value="Task.code" />
      <min value="1" />
    </element>
    <element id="Task.code.coding">
      <path value="Task.code.coding" />
      <min value="1" />
    </element>
    <element id="Task.code.coding.system">
      <path value="Task.code.coding.system" />
      <min value="1" />
      <fixedUri value="http://snomed.info/sct" />
    </element>
    <element id="Task.code.coding.code">
      <path value="Task.code.coding.code" />
      <min value="1" />
      <fixedCode value="308292007" />
    </element>
    <element id="Task.code.coding.display">
      <path value="Task.code.coding.display" />
      <defaultValueString value="Overdracht van zorg" />
    </element>
    <element id="Task.for">
      <path value="Task.for" />
      <min value="1" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://fhir.nl/fhir/StructureDefinition/nl-core-patient" />
      </type>
      <mapping>
        <identity value="eOverdracht-sturen-aanmeldbericht" />
        <map value="e-overdracht-dataelement-v4-4" />
        <comment value="Persoonsgegevens::Patient" />
      </mapping>
      <mapping>
        <identity value="hcim-patient-v3.1-2017EN" />
        <map value="NL-CM:0.1.1" />
        <comment value="Patient" />
      </mapping>
    </element>
    <element id="Task.requester">
      <path value="Task.requester" />
      <min value="1" />
    </element>
    <element id="Task.requester.agent">
      <path value="Task.requester.agent" />
      <definition value="The practitioner or organisation who initiated the task." />
      <type>
        <code value="Reference" />
        <targetProfile value="http://fhir.nl/fhir/StructureDefinition/nl-core-organization" />
      </type>
      <type>
        <code value="Reference" />
        <targetProfile value="http://fhir.nl/fhir/StructureDefinition/nl-core-practitioner" />
      </type>
      <mapping>
        <identity value="eOverdracht-sturen-aanmeldbericht" />
        <map value="e-overdracht-dataelement-v4-102" />
        <comment value="Sturende organisatie::Zorgaanbieder" />
      </mapping>
    </element>
    <element id="Task.requester.agent.extension">
      <path value="Task.requester.agent.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Task.requester.agent.extension:practitionerRole">
      <path value="Task.requester.agent.extension" />
      <sliceName value="practitionerRole" />
      <min value="0" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://nictiz.nl/fhir/StructureDefinition/practitionerrole-reference" />
      </type>
    </element>
    <element id="Task.owner">
      <path value="Task.owner" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://fhir.nl/fhir/StructureDefinition/nl-core-organization" />
      </type>
      <mapping>
        <identity value="eOverdracht-sturen-aanmeldbericht" />
        <map value="e-overdracht-dataelement-v4-153" />
        <comment value="Ontvangende organisatie::Zorgaanbieder" />
      </mapping>
      <mapping>
        <identity value="eOverdracht-sturen-antwoord-aanmeldbericht" />
        <map value="e-overdracht-dataelement-v4-153" />
        <comment value="Ontvangende organisatie::Zorgaanbieder" />
      </mapping>
    </element>
    <element id="Task.input">
      <path value="Task.input" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="type.coding.code" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Task.input:aanmeldbericht">
      <path value="Task.input" />
      <sliceName value="aanmeldbericht" />
      <max value="1" />
      <mapping>
        <identity value="eOverdracht-sturen-aanmeldbericht" />
        <map value="e-overdracht-transaction-63" />
        <comment value="Sturen aanmeldbericht" />
      </mapping>
    </element>
    <element id="Task.input:aanmeldbericht.type.coding.system">
      <path value="Task.input.type.coding.system" />
      <fixedUri value="http://loinc.org" />
    </element>
    <element id="Task.input:aanmeldbericht.type.coding.code">
      <path value="Task.input.type.coding.code" />
      <fixedCode value="57830-2" />
    </element>
    <element id="Task.input:aanmeldbericht.value[x]:valueReference">
      <path value="Task.input.valueReference" />
      <sliceName value="valueReference" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://nictiz.nl/fhir/StructureDefinition/eOverdracht-aanmeldbericht" />
      </type>
    </element>
    <element id="Task.input:overdrachtbericht">
      <path value="Task.input" />
      <sliceName value="overdrachtbericht" />
      <max value="1" />
    </element>
    <element id="Task.input:overdrachtbericht.type.coding.system">
      <path value="Task.input.type.coding.system" />
      <fixedUri value="http://snomed.info/sct" />
    </element>
    <element id="Task.input:overdrachtbericht.type.coding.code">
      <path value="Task.input.type.coding.code" />
      <fixedCode value="371535009" />
    </element>
    <element id="Task.input:overdrachtbericht.type.coding.display">
      <path value="Task.input.type.coding.display" />
      <fixedString value="verslag van overdracht" />
    </element>
    <element id="Task.input:overdrachtbericht.value[x]:valueReference">
      <path value="Task.input.valueReference" />
      <sliceName value="valueReference" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://nictiz.nl/fhir/StructureDefinition/eOverdracht-overdrachtsbericht-volwassenen" />
      </type>
    </element>
    <element id="Task.output">
      <path value="Task.output" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="type.coding.code" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Task.output:alternativeDate">
      <path value="Task.output" />
      <sliceName value="alternativeDate" />
      <max value="1" />
      <mapping>
        <identity value="eOverdracht-sturen-antwoord-aanmeldbericht" />
        <map value="e-overdracht-dataelement-v4-1164" />
        <comment value="Alternatieve datum overplaatsing" />
      </mapping>
    </element>
    <element id="Task.output:alternativeDate.type.coding">
      <path value="Task.output.type.coding" />
      <min value="1" />
      <max value="1" />
    </element>
    <element id="Task.output:alternativeDate.type.coding.system">
      <path value="Task.output.type.coding.system" />
      <min value="1" />
      <fixedUri value="http://snomed.info/sct" />
    </element>
    <element id="Task.output:alternativeDate.type.coding.code">
      <path value="Task.output.type.coding.code" />
      <min value="1" />
      <fixedCode value="146851000146105" />
    </element>
    <element id="Task.output:alternativeDate.type.coding.display">
      <path value="Task.output.type.coding.display" />
      <defaultValueString value="Alternative date for transfer of care (waarneembare entiteit)" />
    </element>
    <element id="Task.output:alternativeDate.value[x]:valueDateTime">
      <path value="Task.output.valueDateTime" />
      <sliceName value="valueDateTime" />
      <type>
        <code value="dateTime" />
      </type>
    </element>
  </differential>
</StructureDefinition>